<!DOCTYPE html>
<html>
<head>
  <title>Finalproject</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
<!--   // <script src="https://maps.googleapis.com/maps/api/js?v=3.exp"></script> -->
  <script type="text/javascript"
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB0B0X06CrlKX4Dr0RrKCnjBIlD6Tpi8xs&sensor=FALSE">
   </script> 
   
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>

  <%= csrf_meta_tags %>
</head>
<body>

<div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
  <div class="container">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <%= link_to "Parking Finder", root_path, :class => "navbar-brand" %>
    </div>
    <% if user_signed_in? %>
 
        <%= button_to 'Logout', destroy_user_session_path, :method => :delete, :type => "button", :class => "btn btn-default navbar-btn btn-success navbar-right"  %>   

        <p class="navbar-text navbar-right sign-in">Signed in as <%= "#{current_user.first_name} #{current_user.last_name}" %></p>  
    <% else %>
    
           <%= render "layouts/sign_in" %>
   
    <% end %>
 
  </div>
</div>

<% if flash[:alert] %>
    <div id="general-alert" class="alert alert-danger" role="alert"><%= flash[:alert] %></div>
<% end %>
<% if flash[:notice] %>  
    <div id="general-alert" class="alert alert-success" role="alert"><%= flash[:notice] %></div>
<% end %>
<%= yield %>



<div class="container text-left">
  <footer>
    <p>&copy; We Got Coders 2014</p>
  </footer>
</div>



</body>
</html>

